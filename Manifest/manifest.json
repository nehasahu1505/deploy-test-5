{
  "$schema": "https://statics.teams.microsoft.com/sdk/v1.3.0-beta.2/manifest/MicrosoftTeams.schema.json",
  "manifestVersion": "1.3",
  "version": "1.0.0",
  "id": "5c0b8ee0-d445-409b-9c19-ab9edc07a314",
  "packageName": "com.microsoft.teams.celebration",
  "developer": {
    "name": "<company name>",
    "websiteUrl": "<website url>",
    "privacyUrl": "<privacy url>",
    "termsOfUseUrl": "<terms of use url>"
  },
  "name": {
    "short": "Celebration bot"
  },
  "description": {
    "short": "Celebration bot",
    "full": "Celebration bot is a Teams bot that helps Team members celebrate each others birthdays, anniversaries and other recurring events"
  },
  "icons": {
    "outline": "outline.png",
    "color": "color.png"
  },
  "accentColor": "#64A2CC",
  "staticTabs": [
    {
      "contentUrl": "https://<app domain>/Tabs/Events?userObjectId={userObjectId}",
      "entityId": "EventsTab",
      "name": "Events",
      "scopes": [
        "personal"
      ]
    }
  ],
  "bots": [
    {
      "botId": "<bot id>",
      "isNotificationOnly": false,
      "scopes": [ "team", "personal" ]
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "<app domain>"
  ]
}