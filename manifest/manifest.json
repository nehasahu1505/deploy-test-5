{
  "$schema": "https://statics.teams.microsoft.com/sdk/v1.3.0-beta.2/manifest/MicrosoftTeams.schema.json",
  "manifestVersion": "1.3",
  "version": "1.3",
  "id": "{{MicrosoftAppId}}",
  "packageName": "com.skype.teams.celebration",
  "developer": {
    "name": "Microsoft",
    "websiteUrl": "https://www.microsoft.com",
    "privacyUrl": "https://www.microsoft.com/privacy",
    "termsOfUseUrl": "https://www.microsoft.com/termsofuse"
  },
  "name": {
    "short": "Celebration app",
    "full": "Celebration app"
  },
  "description": {
    "short": "Celebration app",
    "full": "Celebration app"
  },
  "icons": {
    "outline": "celebration_bot_outline.png",
    "color": "celebration_bot_full-color.png"
  },
  "accentColor": "#3F487F",
  "staticTabs": [
    {
      "contentUrl": "https://{{domain}}/Tabs/Events?userObjectId={userObjectId}",
      "entityId": "EventsTab",
      "name": "Events",
      "scopes": [
        "personal"
      ]
    }
  ],
  "bots": [
    {
      "botId": "{{MicrosoftAppId}}",
      "isNotificationOnly": false,
      "scopes": [ "team", "personal" ]
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "{{domain}}"
  ]
}